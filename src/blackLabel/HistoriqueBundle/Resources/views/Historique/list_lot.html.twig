{# src/blackLabel/HistoriqueBundle/Resources/views/Historique/list_lot.html.twig #}
{% extends "whiteLabelBackOfficeBundle::layout_core.html.twig" %}

{% block stylesheet %}
    {{ parent() }}
    {% stylesheets filter='cssrewrite'
    'bundles/whitelabelbackoffice/css/core/list/*.css'
    'bundles/whitelabelbackoffice/css/lot/historique.css'
    'bundles/blacklabelhistorique/css/historique/list_lot.css'
    %}
    <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
    {% endstylesheets %}
{% endblock %}

{% block content %}
<div class="col-xs-12 col-sm-12 col-md-10 col-lg-10 col-xs-offset-0 col-sm-offset-0 col-md-offset-1 col-lg-offset-1 marginBottom40 marginTop20 paddingBottom15 hidden" id="mainWrapper">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 marginBottom40 noPadding" id="bloc_title">
        <h2>Consulter l'historique d'un lot: {{ lotNumero }}</h2>
        {{ include('whiteLabelBackOfficeBundle:Lot:inc/back.html.twig') }}
    </div>

{{ include('whiteLabelBackOfficeBundle:Lot:inc/historique/table_lot.html.twig') }}

</div>
{% endblock %}

{% block javascript %}
    {{ parent() }}
    {% javascripts
    'bundles/blacklabelhistorique/js/historique/list/*.js'
    'bundles/blacklabelhistorique/js/core/list/jquery_datatable.js'
    'bundles/blacklabelhistorique/js/core/list/bootstrap_datatable.js'
    %}
    <script src="{{ asset_url }}" type="text/javascript"></script>
    {% endjavascripts %}
{% endblock %}
