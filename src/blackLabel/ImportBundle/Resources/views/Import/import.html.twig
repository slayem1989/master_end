{# src/blackLabel/ImportBundle/Resources/views/Import/import.html.twig #}
{% extends "whiteLabelBackOfficeBundle::layout_core.html.twig" %}

{% block stylesheets %}
    {{ parent() }}
    {% stylesheets filter='cssrewrite'
    'bundles/whitelabelbackoffice/css/core/form/*.css'
    'bundles/blacklabelimport/css/import/import.css'
    %}
    <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
    {% endstylesheets %}
{% endblock %}

{% block contents %}
<div class="col-xs-12 col-sm-12 col-md-8 col-lg-6 col-xs-offset-0 col-sm-offset-0 col-md-offset-2 col-lg-offset-3 marginBottom40 hidden" id="content-wrapper">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <div id="container-title">
            <h2>Importer des donn&eacute;es</h2>
        </div>
    </div>

{{ include('blackLabelImportBundle:Import:inc/form/file.html.twig') }}

</div>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    {% javascripts
    'bundles/blacklabelimport/js/import/*.js'
    %}
    <script src="{{ asset_url }}" type="text/javascript"></script>
    {% endjavascripts %}
{% endblock %}
